
    public void zdvojBod( ) {
        if ( jePoleNula( xP ) || jePoleNula( zP ) ) { // 5.
            // Vysledek bude bod v nekonecnu
            priradPolynom( xP, polynomJedna );
            priradPolynom( yP, polynomJedna );
            priradPolynom( zP, polynomNula );
            return;
        }

        byte[] T1 = vytvorKopiiPolynomu( xP );   // 1.               
        byte[] T2 = vytvorKopiiPolynomu( yP );   // 2.
        byte[] T3 = vytvorKopiiPolynomu( zP );   // 3.
        byte[] T4 = vytvorKopiiPolynomu( ECc );  // 4.

        prinasobPolynom( T2, T3 );     // 6.
        umocniPolynom( T3 );           // 7.
        prinasobPolynom( T4, T3 );     // 8.
        prinasobPolynom( T3, T1 );     // 9. // v T3 je Z

        prictiPolynom( T2, T3 );       // 10.
        prictiPolynom( T4, T1 );       // 11.
        umocniPolynom( T4 );           // 12.
        umocniPolynom( T4 );           // 13. // v T4 je X

        umocniPolynom( T1 );           // 14.
        prictiPolynom( T2, T1 );       // 15. // v T2 je U

        prinasobPolynom( T2, T4 );     // 16.
        umocniPolynom( T1 );           // 17.

        prinasobPolynom( T1, T3 );     // 18.
        prictiPolynom( T2, T1 );       // 19. // v T1 je Y

        priradPolynom( xP, T4 );       // 21.
        priradPolynom( yP, T2 );       // 22.
        priradPolynom( zP, T3 );       // 23.

    }

